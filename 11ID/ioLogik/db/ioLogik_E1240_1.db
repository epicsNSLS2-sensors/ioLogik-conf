# Template for I/O statistics

record(bo,"$(Sys)$(Dev)AIHistEnable") {
    field(DTYP,"asynUInt32Digital")
    field(OUT,"@asynMask($(ASYNPORT)_AI 0 0xFFFF)ENABLE_HISTOGRAM")
    field(ZNAM,"Disable")
    field(ONAM,"Enable")
}

record(bo, "$(Sys)$(Dev)AIStatistics") {
    field(SCAN, "10 second")
    field(FLNK, "$(Sys)$(Dev)AIHist.PROC")
}

record(waveform,"$(Sys)$(Dev)AIHist") {
    field(DTYP,"asynInt32ArrayIn")
    field(INP,"@asyn($(ASYNPORT)_AI 0)READ_HISTOGRAM")
    field(FTVL,"LONG")
    field(NELM,"200")  # This number should match driver
    field(FLNK,"$(Sys)$(Dev)AIHistTimeAxis.PROC")
}

record(waveform,"$(Sys)$(Dev)AIHistTimeAxis") {
    field(DTYP,"asynInt32ArrayIn")
    field(INP,"@asyn($(ASYNPORT)_AI 0)HISTOGRAM_TIME_AXIS")
    field(FTVL,"LONG")
    field(NELM,"200")  # This number should match driver
    field(FLNK,"$(Sys)$(Dev)AIReadOK.PROC")
}

record(longout,"$(Sys)$(Dev)AIMsPerBin") {
    field(DTYP,"asynInt32")
    field(OUT,"@asyn($(ASYNPORT)_AI 0)HISTOGRAM_BIN_TIME")
    field(DRVL,"0")
    field(VAL,"1")
    field(PINI,"1")
}

record(longin,"$(Sys)$(Dev)AIReadOK") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AI 0)READ_OK")
    field(FLNK,"$(Sys)$(Dev)AIWriteOK.PROC")
}

record(longin,"$(Sys)$(Dev)AIWriteOK") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AI 0)WRITE_OK")
    field(FLNK,"$(Sys)$(Dev)AIIOErrors.PROC")
}

record(longin,"$(Sys)$(Dev)AIIOErrors") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AI 0)IO_ERRORS")
    field(FLNK,"$(Sys)$(Dev)AILastIOTime.PROC")
}

record(longin,"$(Sys)$(Dev)AILastIOTime") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AI 0)LAST_IO_TIME")
    field(FLNK,"$(Sys)$(Dev)AIMaxIOTime.PROC")
}

record(longin,"$(Sys)$(Dev)AIMaxIOTime") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AI 0)MAX_IO_TIME")
}
