# Template for I/O statistics

record(bo,"$(Sys)$(Dev)AO-SPHistEnable") {
    field(DTYP,"asynUInt32Digital")
    field(OUT,"@asynMask($(ASYNPORT)_AO 0 0xFFFF)ENABLE_HISTOGRAM")
    field(ZNAM,"Disable")
    field(ONAM,"Enable")
}

record(bo, "$(Sys)$(Dev)AO-SPStatistics") {
    field(SCAN, "10 second")
    field(FLNK, "$(Sys)$(Dev)AO-SPHist.PROC")
}

record(waveform,"$(Sys)$(Dev)AO-SPHist") {
    field(DTYP,"asynInt32ArrayIn")
    field(INP,"@asyn($(ASYNPORT)_AO 0)READ_HISTOGRAM")
    field(FTVL,"LONG")
    field(NELM,"200")  # This number should match driver
    field(FLNK,"$(Sys)$(Dev)AO-SPHistTimeAxis.PROC")
}

record(waveform,"$(Sys)$(Dev)AO-SPHistTimeAxis") {
    field(DTYP,"asynInt32ArrayIn")
    field(INP,"@asyn($(ASYNPORT)_AO 0)HISTOGRAM_TIME_AXIS")
    field(FTVL,"LONG")
    field(NELM,"200")  # This number should match driver
    field(FLNK,"$(Sys)$(Dev)AO-SPReadOK.PROC")
}

record(longout,"$(Sys)$(Dev)AO-SPMsPerBin") {
    field(DTYP,"asynInt32")
    field(OUT,"@asyn($(ASYNPORT)_AO 0)HISTOGRAM_BIN_TIME")
    field(DRVL,"0")
    field(VAL,"1")
    field(PINI,"1")
}

record(longin,"$(Sys)$(Dev)AO-SPReadOK") {
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(ASYNPORT)_AO 0)READ_OK")
    field(FLNK,"$(Sys)$(Dev)AO-SPWriteOK.PROC")
}

record(longin,"$(Sys)$(Dev)AO-SPWriteOK") {
    field(DTYP,"asynInt32")
